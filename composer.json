{
  "name": "abterphp/framework",
  "description": "Foundations for AbterPHP",
  "authors": [
    {
      "name": "Peter Aba",
      "email": "peter@peteraba.com",
      "role": "Engineer"
    }
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AbterPhp\\Framework\\": [
        "src",
        "tests"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "ext-iconv": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "monolog/monolog": "1.22.0",
    "opulence/opulence": "*",
    "filp/whoops": "^2.1",
    "league/flysystem": "^1.0",
    "cocur/slugify": "^3.1",
    "casbin/casbin": "^0.1.6",
    "casbin/database-adapter": "^0.2.0",
    "matthiasmullie/minify": "^1.3",
    "swiftmailer/swiftmailer": "^6.1",
    "mikey179/vfsstream": "^1.6",
    "bjeavons/zxcvbn-php": "^0.4",
    "dflydev/apache-mime-types": "^1.0",
    "psr/http-message": "^1.0",
    "nyholm/psr7": "^1.1",
    "league/oauth2-server": "^7.4",
    "nyholm/psr7-server": "^0.3.0",
    "hackzilla/password-generator": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "squizlabs/php_codesniffer": "^3.4",
    "phpmd/phpmd": "^2.6",
    "friendsofphp/php-cs-fixer": "^2.14",
    "phan/phan": "^2.2"
  },
  "suggest": {
    "ext-redis": "Allows better caching"
  },
  "config": {
    "prefer-install": "dist"
  }
}
